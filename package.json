{
  "name": "@gsx/pancake-swap-mock",
  "version": "1.0.0",
  "repository": "https://github.com/gentasumantri/pancake-swap-mock",
  "author": "Genta Sumantri <gentaaaa@outlook.co.id>",
  "license": "MIT",
  "scripts": {
    "compile": "truffle compile",
    "deploy": "truffle migrate",
    "deploy:reset": "truffle migrate --reset",
    "deploy:live": "truffle migrate --network live | tee logs/migrations_live.log",
    "deploy:live:reset": "truffle migrate --network live --reset | tee logs/migrations_live.log",
    "deploy:testnet": "truffle migrate --network testnet | tee logs/migrations_testnet.log",
    "deploy:testnet:reset": "truffle migrate --network testnet --reset | tee logs/migrations_testnet.log",
    "lint": "yarn lint:js && yarn lint:sol",
    "lint:fix": "yarn lint:js:fix && yarn lint:sol:fix",
    "lint:sol": "solhint contracts/**/*.sol",
    "lint:sol:fix": "solhint contracts/**/*.sol --fix",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:js:fix": "eslint . --ignore-path .gitignore --fix",
    "prettier": "yarn prettier:js && yarn prettier:sol",
    "prettier:fix": "yarn prettier:js:fix && yarn prettier:sol:fix",
    "prettier:js": "prettier --check **.js --ignore-path .gitignore",
    "prettier:js:fix": "prettier --check **.js --ignore-path .gitignore --write",
    "prettier:sol": "prettier --check contracts/**/*.sol",
    "prettier:sol:fix": "prettier --check contracts/**/*.sol --write",
    "reset": "rimraf artifacts build cache mochawesome-report coverage coverage.json"
  },
  "devDependencies": {
    "@gsx/bsc-genesis": "https://github.com/gentasumantri/bsc-genesis.git",
    "@openzeppelin/contracts": "3.4.0",
    "@truffle/hdwallet-provider": "^1.4.1",
    "@uniswap/lib": "^4.0.1-alpha",
    "dotenv": "^10.0.0",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.15",
    "solhint": "^3.3.6",
    "truffle": "^5.4.0"
  }
}
